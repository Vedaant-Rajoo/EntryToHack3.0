<% include ./partials/header%>
        <div class="col-md-12">
             <% if(noMatch !== null) {%>
                <h3><%= noMatch %></h3>
                 <p>
             </p>
             <% } %>
         </div>
          <table class="table-striped table table-bordered">
            <tr class="thead-dark">
              <th>Title</th>
              <th>Category</th>
              <th>Description</th>
              <th>Mentor Name</th>
              <th>Applicants</th>
            </tr>
                <% challenges.forEach(function(challenge){
                challenge.mentorChallenges.forEach(function(chall) {
                    console.log(chall);%>
                <tr>
                  <td><%= chall.title %></td>
                  <td><%= chall.category %></td>
                  <td><%= chall.description %></td>
                  <td><%= challenge.name %></td>
                  <td>
                    <ul>
                        <% for(var i = 0; i < chall.applicants.length; i++){ %>
                          <li>
                            <%= chall.applicants[i];%>
                          </li>
                          <% } %>
                    </ul> 
                  </td>
                  </tr>
                  <% }); %>
        <% }); %>
          </table>   
<% include ./partials/footer%>
