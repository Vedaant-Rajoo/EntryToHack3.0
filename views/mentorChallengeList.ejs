        <div class="col-md-12">
             <% if(noMatch !== null) {%>
                <h3><%= noMatch %></h3>
                 <p>
             </p>
             <% } %>
         </div>
          <table class="table-striped table table-bordered">
            <tr class="thead-dark">
              <th>Title</th>
              <th>Category</th>
              <th>Description</th>
              <th>Mentor Name</th>
              <th>Applicants</th>
            </tr>
                <% challenges.forEach(function(challenge){ %>
                <tr>
                  <td><%= challenge.title %></td>
                  <td><%= challenge.category %></td>
                  <td><%= challenge.description %></td>
                  <td><%= challenge.mentor %></td>
                  <td>
                    <ul>
                        <% for(var i = 0; i < challenge.applicants.length; i++){ %>
                          <li>
                            <%= challenge.applicants[i];%>
                          </li>
                          <% } %>
                    </ul> 
                  </td>           
        <% }); %>
          </table>   